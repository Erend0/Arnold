<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NEA.Regenerate">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="DaysAvailable"
                Text="Please Input the days available"   
             />
            
            <Entry x:Name="HoursAvailable "
                Text="Please Input the time avaible"   
             />

            <Button x:Name="TimeSubmit"
               Text="Generate"
               CornerRadius="10"
               Margin="10"
               Clicked="TimeSubmit_Clicked"
             />
            
            


        </StackLayout>
    </ContentPage.Content>
</ContentPage>